{
    "name": "al-tool",
    "private": true,
    "version": "0.0.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:api": "npm --workspace=apps/api run dev",
        "build:api": "npm --workspace=apps/api run build",
        "dev:electron": "npm --workspace=apps/desktop run dev",
        "build:electron": "npm --workspace=apps/desktop run build",
        "install:all": "npm install"
    },
    "devDependencies": {
        "electron": "^39.2.4",
        "electron-builder": "^26.0.12"
    }
}
